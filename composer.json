{
    "name": "wg-hyve/cloak-port",
    "description": "Guard; Checks JWTs against different guard types; default keycloak & passport",
    "keywords": [
        "laravel",
        "keycloak",
        "passport"
    ],
    "homepage": "https://github.com/wg-hyve/cloak-port",
    "license": "MIT",
    "authors": [
        {
            "name": "Sinema"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "laravel/passport": "^12.0",
        "firebase/php-jwt": "^6.8",
        "wg-hyve/keycloak-laravel-guard": "^0.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.5",
        "guzzlehttp/promises": "^2.0",
        "guzzlehttp/guzzle": "^7.7"
    },
    "autoload": {
        "psr-4": {
            "CloakPort\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClockPort\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CloakPort\\GuardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
