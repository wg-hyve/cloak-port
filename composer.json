{
    "name": "wg-hyve/cloak-port",
    "description": "Guard; Checks JWTs against different guard types; default keycloak & passport",
    "keywords": [
        "laravel",
        "keycloak",
        "passport"
    ],
    "homepage": "https://github.com/wg-hyve/cloak-port",
    "license": "MIT",
    "authors": [
        {
            "name": "Sinema"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.1.0",
        "laravel/passport": "^11.6",
        "robsontenorio/laravel-keycloak-guard": "dev-pre",
        "firebase/php-jwt": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "CloakPort\\": "src/"
        }
    },
    "autoload-dev": {},
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "CloakPort\\GuardServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wg-hyve/laravel-keycloak-guard.git"
        }
    ],
    "minimum-stability": "dev"
}
